<% provide(:title, 'Rental History') %>
<h1>Rental History</h1>
<table class="table table-success table-hover">
  <thead>
    <tr>
      <%# <th>STT</th> %>
      <th>Name Book</th>
      <th>Price</th>
      <th>Quantity</th>
      <!-- Các cột khác (nếu có) -->
    </tr>
  </thead>
  <tbody>
    <% @history.each do |history| %>
      <tr>
        <td><%= history.book.name_book %></td>
        <td><%= history.price %></td>
        <td><%= history.quantity %></td>
      </tr>
    <% end %>
  </tbody>
</table>